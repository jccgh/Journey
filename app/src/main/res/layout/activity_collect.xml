<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_collect"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="jcc.journey.view.activity.CollectActivity">
    <jcc.mycustomtopview.MyCustomTopView
        android:layout_width="match_parent"
        android:layout_height="48dp"
        fresco:leftImgView="@drawable/aditop"
        android:paddingTop="12dp"
        android:paddingBottom="12dp"
        android:id="@+id/collect_customtoview"
        fresco:centerTextTitle="收藏"
        />
    <ListView
        android:id="@+id/collect_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/collect_customtoview"/>
</RelativeLayout>
